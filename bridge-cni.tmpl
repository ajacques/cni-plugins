{
  "cniVersion": "0.3.1",
  "name": "dhcp-cni-network",
  "plugins": [
    {
      "type": "macvlan",
      "name": "macvlan",
      "master": "MASTER_IFACE",
      "ipam": {
        "type": "dhcp"
      }
    },
    {
      "type": "tuning",
      "sysctl": {
        "net.ipv6.conf.eth0.autoconf": "1",
        "net.ipv6.conf.default.autoconf": "1",
        "net.ipv6.conf.eth0.accept_ra": "1",
        "net.ipv6.conf.default.accept_ra": "1"
      }
    },
    {
      "type": "route-fix",
      "master": "MASTER_IFACE"
    }
  ]
}
